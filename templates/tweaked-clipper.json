{
	"schemaVersion": "0.1.0",
	"name": "Tweaked",
	"behavior": "create",
	"noteContentFormat": "{{contentHtml|markdown}}",
	"properties": [
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{author|split:\\\", \\\"|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{published|date:\\\"YYYY-MM-DD HH:mm:ss\\\"}}",
			"type": "date"
		},
		{
			"name": "created",
			"value": "{{date|date:\\\"YYYY-MM-DD HH:mm:ss\\\"}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings, {{schema:@Type:key|kebab}}, {{site|kebab}}, {{schema:author|kebab}}, {{meta:name:keywords}}, ",
			"type": "multitext"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		}
	],
	"triggers": [],
	"noteNameFormat": "{{title|safe_name}}",
	"path": "Clippings",
	"context": "{{contentHtml|markdown}}"
}